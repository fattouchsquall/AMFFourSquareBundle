{
    "name": "amf/foursquare-bundle",
    "description": "A bundle for integrating foursquare API",
    "type": "symfony-bundle",
    "keywords": ["FourSquare", "Venue"],
    "homepage": "https://github.com/fattouchsquall",
    "license": "MIT",
    "authors": [
        {
            "name": "Amine Fattouch",
            "email": "amine.fattouch@gmail.com",
            "role": "Project owner"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "symfony/serializer": "~2.0",
        "symfony/console": "~2.0"
    },
    "require-dev": {
        "friendsofsymfony/jsrouting-bundle": "1.1.*"
    },
    "autoload": {
        "psr-0" : { "AMF\\FourSquareBundle" : "" }
    },
    "minimum-stability": "stable",
    "target-dir": "AMF/FourSquareBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
